{
  "name": "Okta Alpha",
  "description": "Alphabetizes Okta tiles",
  "version": "1.0",
  "manifest_version": 3,
  "author": "Joe Miller",
  "content_scripts": [
      {
          "matches": [
              "https://*.okta.com/app/UserHome"
          ],
          "js": [
              "content.js"
          ],
          "run_at": "document_end"
      }
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
